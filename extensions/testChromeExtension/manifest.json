{
	"manifest_version": 2,
	"name": "My Cool Test Extension",
	"version": "0.1",

	"browser_action": {
		"default_icon": "favicon-32x32.png"
	},

	"background": {
		"scripts": ["background.js"]
	},

	"content_scripts": [
		{
			"matches": ["<all_urls>"],
			"js": ["jquery-2.2.1.min.js", "content.js"]
		}
	]
}